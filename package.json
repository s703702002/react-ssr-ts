{
  "name": "react-ssr-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server/server.ts",
    "clean": "rm -rf dist",
    "build": "concurrently 'npm run build:*'",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "prebuild:server": "vite build --ssr src/entry-server.tsx",
    "build:server": "node build-server.mjs",
    "lint": "concurrently 'npm run lint:*'",
    "lint:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:tsc": "tsc",
    "preview": "NODE_ENV=production node dist/server.cjs"
  },
  "dependencies": {
    "antd": "^5.18.0",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^8.2.2",
    "esbuild": "0.21.3",
    "esbuild-node-externals": "^1.13.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "tslib": "^2.6.2",
    "tsx": "^4.10.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
